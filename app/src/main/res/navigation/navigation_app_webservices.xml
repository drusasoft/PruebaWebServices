<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation_app_webservices"
    app:startDestination="@id/fragmentMenuPrincipal">

    <fragment
        android:id="@+id/fragmentMenuPrincipal"
        android:name="com.aar.pruebawebservices.fragments.FragmentMenuPrincipal"
        android:label="FragmentMenuPrincipal">

        <action
            android:id="@+id/irFragmentWSPersonas"
            app:destination="@id/fragmentWSPersonas"/>

        <action
            android:id="@+id/irFragmentWSConsultarIP"
            app:destination="@id/fragmentWSConsultarIP"/>

        <action
            android:id="@+id/irFragmentConsultarHoraMundial"
            app:destination="@id/fragmentConsultarHoraMundial"/>
        <action
            android:id="@+id/irFragmentWSConversorMoneda"
            app:destination="@id/fragmentWSConversorMoneda" />

    </fragment>

    <fragment
        android:id="@+id/fragmentWSPersonas"
        android:name="com.aar.pruebawebservices.fragments.FragmentWSPersonas"
        android:label="FragmentWSPersonas">

        <action
            android:id="@+id/irFragmentZoomImagen"
            app:destination="@id/fragmentZoomImagen"/>

        <action
            android:id="@+id/irFragmentDetallesPersona"
            app:destination="@id/fragmentDetallesPersona" />

    </fragment>

    <fragment
        android:id="@+id/fragmentZoomImagen"
        android:name="com.aar.pruebawebservices.fragments.FragmentZoomImagen"
        android:label="FragmentZoomImagen"/>

    <fragment
        android:id="@+id/fragmentDetallesPersona"
        android:name="com.aar.pruebawebservices.fragments.FragmentDetallesPersona"
        android:label="FragmentDetallesPersona">

        <action
            android:id="@+id/irFragmentZoomImagenDetelles"
            app:destination="@id/fragmentZoomImagen"/>

    </fragment>

    <fragment
        android:id="@+id/fragmentWSConsultarIP"
        android:name="com.aar.pruebawebservices.fragments.FragmentWSConsultarIP"
        android:label="FragmentWSConsultarIP">

        <action
            android:id="@+id/irFragmentDetallesIP"
            app:destination="@id/fragmentDetallesIP"/>

    </fragment>

    <fragment
        android:id="@+id/fragmentDetallesIP"
        android:name="com.aar.pruebawebservices.fragments.FragmentDetallesIP"
        android:label="FragmentDetallesIP"/>

    <fragment
        android:id="@+id/fragmentConsultarHoraMundial"
        android:name="com.aar.pruebawebservices.fragments.FragmentWSConsultarHoraMundial"
        android:label="FragmentConsultarHoraMundial">

        <action
            android:id="@+id/irFragmentDetallesHora"
            app:destination="@id/fragmentDetallesHora"/>

    </fragment>

    <fragment
        android:id="@+id/fragmentDetallesHora"
        android:name="com.aar.pruebawebservices.fragments.FragmentDetallesHora"
        android:label="FragmentDetallesHora"/>

    <fragment
        android:id="@+id/fragmentWSConversorMoneda"
        android:name="com.aar.pruebawebservices.fragments.FragmentWSConversorMoneda"
        android:label="FragmentWSConversorMoneda" >

        <action
            android:id="@+id/irFragmentDetallesConversorMoneda"
            app:destination="@id/fragmentDetallesConversorMoneda"/>

    </fragment>

    <fragment
        android:id="@+id/fragmentDetallesConversorMoneda"
        android:name="com.aar.pruebawebservices.fragments.FragmentDetallesConversorMoneda"
        android:label="FragmentDetallesConversorMoneda"/>

</navigation>